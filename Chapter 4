//4.6
struct CandyBar{
  char brand[20];
  float weight;
  int calory;
};
CandyBar snacks[3] = {
    {"Mocha Munch", 2.3, 350},
    {"Mocha Munch", 2.3, 350},
    {"Mocha Munch", 2.3, 350}
};
  cout<<snacks[0].brand<<endl;
  cout<<snacks[0].weight<<endl;
  cout<<snacks[0].calory<<endl;
  return 0 ;
}
